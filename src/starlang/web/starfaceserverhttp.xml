						var info = JSON.parse(event.getParam('eventdata').toString());
						context.no.setValue(info.payload.no);
						context.srcdevice.setValue(info.payload.srcdevice);
						context.dstdevice.setValue(info.payload.dstdevice);
						context.srcport.setValue(info.payload.srcport);
						context.dstport.setValue(info.payload.dstport);
					]]></script>
					<function name="updateLink" class="LinkManager">
						<parameters>
							<string name="no" in="context.no.toString()"/>
							<string name="srcdevice" in="context.srcdevice.toString()"/>
							<string name="dstdevice" in="context.dstdevice.toString()"/>
							<string name="srcport" in="context.srcport.toString()"/>
							<string name="dstport" in="context.dstport.toString()"/>
						</parameters>
					</function>
					<exit/>
				</then>
			</if>
			<if cond="context.type.toString()=='deleteLink'">
				<then>
					<string name="no"/>
					<script><![CDATA[
						var info = JSON.parse(event.getParam('eventdata').toString());
						context.no.setValue(info.payload.no);
					]]></script>
					<function name="deleteLink" class="LinkManager">
						<parameters>
							<string name="no" in="context.no.toString()"/>					
						</parameters>
					</function>
					<exit/>
				</then>
			</if>
			<if cond="context.type.toString()=='getLink'">
				<then>
					<string name="no"/>
