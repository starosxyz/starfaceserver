<?xml version="1.0" encoding="UTF-8"?>
<starlang  scope="application">
	<function name="startstarfaceserverprocess">
		<function name="LoadProject">
			<parameters>
				<string name="strpath" in="'/opt/staros.xyz/starfaceserver/'"/>
			</parameters>
		</function>
		<function>
			<log>Start StarCore System Process</log>
			<function name="StartProcess">
				<parameters>
					<string name="processname" in="'STARFACESERVER'"/>
					<string name="autostart" in="'Disable'"/>
					<string name="version" in="'1.2.2'"/>
					<string name="category" in="'Drivers'"/>
					<string name="origin" in="'StarOS.xyz'"/>
					<string name="title" in="'Default Apps'"/>
					<string name="desc" in="'Default Apps'"/>
					<string name="url" in="'http://www.staros.xyz'"/>
				</parameters>
			</function>	
		</function>
	</function>
</starlang>